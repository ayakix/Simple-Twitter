{
  "name": "Simple Twitter",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": { "48": "icon48.png", "128": "icon128.png" },
  "description": "",
  "web_accessible_resources": ["css/*.css"],
  "content_scripts": [{
    "matches": ["https://twitter.com/*", "https://*.twitter.com/*"],
    "css": [],
    "js": [
      "lib/jquery.min.js",
      "myapp.js"
    ],
    "run_at":"document_start"
  }]
}

